<%- include("partials/header") -%>

  <h1>Compose</h1>
  <form class="" action="/compose" method="post">
    <div class="form-group">
      <label for="composeTitle" class="form-label">Title</label>
      <input type="text" class="form-control" id="composeTitle" name="composeTitle">
    </div>
    <div class="form-group mb-3">
      <label for="composePost" class="form-label">Post</label>
      <textarea id="composePost" class="form-control" rows="5" name="composePost"></textarea>
    </div>
    <button type="submit" class="btn">Publish</button>
       
    
  </form>

  <%- include("partials/footer") -%>